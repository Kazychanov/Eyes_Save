<Window x:Class="KazychanovEyesSave.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KazychanovEyesSave"
        mc:Ignorable="d"
        Title="Kazychanov" Height="600" Width="1000" FontFamily="Century Gothic" Icon="img\Глазки-save.ico">
  <Grid ShowGridLines="True" Background="#FFE9F9">
    <Grid.RowDefinitions>
      <RowDefinition Height="75"/>
      <RowDefinition Height="*"/>
    </Grid.RowDefinitions>
    <Image Source="img\Глазки-save.png" HorizontalAlignment="Left"></Image>
    <TextBlock Text="Список агентов" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered"
           NavigationUIVisibility="Hidden"/>
    <Button Content="Назад" Name="BtnBack" HorizontalAlignment="Right" Margin="5"
            Click="BtnBack_Click"/>
    
  </Grid>
</Window>
